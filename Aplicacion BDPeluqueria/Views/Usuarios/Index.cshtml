
@model IEnumerable<Aplicacion_BDPeluqueria.Models.CuentaUsuarios.mUsuarios>

@{
    ViewBag.Title = "Index";
}

<div class="form-horizontal">
    <div class="container-fluid">
        <div class="col-md-7">
            <div class="card-header">
                <h2 class="card-title"> Datos de los Usuarios </h2>
            </div>

            <div class="card">
                <div class="form-group">

                    <table class="table">
                        <tr>
                            <th>
                                <span> @Html.Label("Usuario") </span>
                            </th>
                            <th>
                                <span> @Html.Label("Correo electrónico") </span>
                            </th>
                            
                            <th></th>
                        </tr>

                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => item.usuario)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.email)
                                </td>
                                <td> </td>
                            </tr>
                        }

                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
